void setup() {
  size(240, 440); // w: j x 20+2 | h: i x 20+2
}

void draw() {
  for (int i = 0; i < 22; i++) {
    for (int j = 0; j < 12; j++) {
      int now = grid[i][j];
      fill(c[now]);
      rect(j * 20, i * 20, 20, 20);
    }
  }
}

